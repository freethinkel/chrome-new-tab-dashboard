{
	"name": "Chrome dashboard",
	"version": "0.1.0",
	"manifest_version": 2,
	"icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
	"permissions": [
		"storage",
		"topSites",
		"system.cpu",
		"system.memory",
		"system.storage",
		"power",
		"tts",
		"ttsEngine",
		"geolocation",
		"history",
		"downloads",
		"bookmarks",
		"alarms",
		"activeTab",
		"*://*/*"
	],
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval' 'unsafe-inline'; object-src 'self'"
}
