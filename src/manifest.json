{
	"name": "chrome dashboard",
	"version": "0.1.0",
	"manifest_version": 2,
	"permissions": ["storage", "*://*/*"],
	"host_permissions": ["*://*/*"],
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval' 'unsafe-inline'; object-src 'self'"
}
